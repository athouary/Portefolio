<!DOCTYPE html>
<html>
    <head>
    {% block head %}

        <title>{% block title %}PMD Static{% endblock %}</title>

        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/10up-sanitize.css/4.1.0/sanitize.min.css">

        {% if env_serv is defined and env_serv == 'production' %}
            <!-- Latest compiled and minified CSS -->
            <link rel="stylesheet" href="/assets/styles/common.css?71606ec1a31f4111f94e3c8fbd3920bb&3f68af99ec362c111a18">
            <link rel="stylesheet" href="/assets/styles/main.css?da5c104323f819529f72e913020d92ba&3f68af99ec362c111a18">
         {% endif %}
    {% endblock %}
    </head>
    <body class="body">
        <h1>Layout</h1>
        {% block header %}
            Layout Header
        {% endblock %}

        {% block body %}
            In Layout Body
        {% endblock %}

        <script src="/assets/scripts/common.js?85232310f56a28f18ac1&3f68af99ec362c111a18"></script>
        <script src="/assets/scripts/main.js?4cd0cdf595f6c7b7c3ca&3f68af99ec362c111a18"></script>

        {% block javascript %}
        {% endblock %}

    </body>
</html>

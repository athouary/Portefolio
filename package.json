{
  "name": "pmd-starterpack",
  "version": "0.0.8",
  "description": "Complete Front-End workflow with Webpack, Babel, PostCss and real php Twig",
  "private": true,
  "homepage": "https://bitbucket.org/prismamediadigital/pmd-starterpack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/prismamediadigital/pmd-starterpack"
  },
  "author": "PMD",
  "engines": {
    "node": "^6",
    "npm": "^3.3.6"
  },
  "config": {
    "baseBuild": "./src",
    "staticRoute": "./src",
    "fontRoute": "./src/assets/fonts"
  },
  "scripts": {
    "server": "node server",
    "server:open": "node server open",
    "clean:build": "rimraf -- build",
    "build:font": "dcs-icon-font --icons './telfront2/static/assets/fonts/svg/**/*.svg' --out $npm_package_config_fontRoute --fontname TEL --baseclass icon --classprefix icon- --cssTemplate $npm_package_config_fontRoute/font.hbs --cssDest $npm_package_config_staticRoute/assets/styles/ --cssFontsUrl ../fonts/TEL/",
    "build:prod": "npm run clean:build && webpack --progress --colors --config webpack.prod.config.js",
    "build:dev": "npm run clean:build && webpack --progress --colors --config webpack.dev.config.js",
    "eslint": "./node_modules/.bin/eslint",
    "eslint:test": "eslint -c .eslintrc ./src/ || true",
    "eslint:format": "eslint -c .eslintrc --fix ./src/ || true",
    "stylelint": "./node_modules/.bin/stylelint",
    "stylelint:test": "stylelint './src/**/*.css' --config .stylelintrc || true",
    "stylefmt": "./node_modules/.bin/stylefmt",
    "stylefmt:format": "stylefmt -c .stylelintrc -R ./src/"
  },
  "dependencies": {
      "jquery": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.14.0",
    "css-loader": "^0.23.1",
    "css-mqpacker": "^5.0.1",
    "dcs-icon-font": "^2.2.0",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^3.21.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^2.0.0",
    "node-twig": "^1.1.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-neat": "^2.5.2",
    "postcss-reporter": "^1.4.1",
    "postcss-url": "^5.1.2",
    "rimraf": "^2.5.4",
    "request": "^2.74.0",
    "style-loader": "^0.13.1",
    "stylefmt": "^4.2.3",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}

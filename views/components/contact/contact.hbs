<section>
  <h3 class="contact-title">
    {{contact.title}}
  </h3>
  <form class="contact-form" method="post" action="">
    <div>
      <div class="contact-name">
        <img src="{{contact.name.image.url}}" alt="{{contact.name.image.title}}"/>
        <input type="text" placeholder="{{contact.name.placeholder}}"/>
      </div>
      <div class="contact-object">
        <img src="{{contact.object.image.url}}" alt="{{contact.object.image.title}}"/>
        <input type="text" placeholder="{{contact.object.placeholder}}"/>
      </div>
    </div>
    <div class="contact-message">
      <img src="{{contact.message.image.url}}" alt="{{contact.message.image.title}}"/>
      <textarea name="name" rows="8" cols="80" type="text" placeholder="{{contact.message.placeholder}}"></textarea>
    </div>
    <button type="submit">{{contact.button}}</button>
  </form>
</section>
